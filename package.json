		"@graphql-codegen/typescript-operations": "^4.0.1",
		"@graphql-codegen/typescript-resolvers": "^2.7.13",
		"@parcel/watcher": "^2.4.1",
		"@types/node": "^22.8.1",
		"@types/react": "^18.3.12",
		"@types/react-dom": "^18.3.1",
		"chokidar-cli": "^3.0.0",
		"eslint": "^9.13.0",
		"eslint-config-next": "^12.3.4",
		"graphql-config": "^5.1.3",
		"tsx": "^4.19.1"
	}
